                console.log("promotedfeed: ", result);
                if (result.success == 1) {
                    this.promotedfeedData = result.data;
                }
                else {
                    this.promotedfeedData.banner_image = result.noDataImage;
                }
            }, err => {
                this.apiService.showToastMessage(JSON.stringify(err), 'top', 3000, 'redBg');
            });
        });
    }
